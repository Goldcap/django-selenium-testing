<table>
<thead>
 <tr>
  <td>
  &nbsp;
  </td>
</tr>
</thead>
<tbody>
{{#each user in controller.paginatedContent}}
 <tr>
  <td>
        {{user.id}}
  </td>
  
  
  <td>
        {{user.password}}
  </td>
  
  
  <td>
        {{user.last_login}}
  </td>
  
  
  <td>
        {{user.is_superuser}}
  </td>
  
  
  <td>
        {{user.username}}
  </td>
  
  
  <td>
        {{user.first_name}}
  </td>
  
  
  <td>
        {{user.last_name}}
  </td>
  
  
  <td>
        {{user.email}}
  </td>
  
  
  <td>
        {{user.is_staff}}
  </td>
  
  
  <td>
        {{user.is_active}}
  </td>
  
  
  <td>
        {{user.date_joined}}
  </td>
  
  
  <td>
  {{#link-to "user" user}}
  <button class="tiny"><i class="fa fa-pencil-square-o"></i> Edit</button>
  {{/link-to}}
  </td> 
</tr>
{{/each}}
</tbody>
</table>

{{pagination-links model='users' page=page num_pages=num_pages}}